server:
  port:

spring:
  config:

    # import config file having database information
    import: database.properties
  datasource:
    url: ${db.url}
    username: ${db.username}
    password: ${db.password}

    # driver name
    driver-class-name: ${db.driver}
  jpa:
    hibernate:
      ddl-auto:

    # dialect setting
    database-platform: ${db.dialect}
    properties:
      hibernate:
        show_sql:
        format_sql: